{
  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": [
    [
      "i18next-extract",
      {
        "locales": ["en", "ru"],
        "keySeparator": false,
        "outputPath": "public/locales/{{locale}}/translation.json",
        "keyAsDefaultValue": true
      }
    ]
  ]
}
